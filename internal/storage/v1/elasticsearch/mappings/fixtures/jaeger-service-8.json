{
  "priority": 501,
  "index_patterns": ["test-jaeger-service*", "test-jaeger-ds-service*"],
  "data_stream": {},
  "template": {
    "aliases": {
      "test-jaeger-service-read": {}
    },
    "settings": {
      "index.number_of_shards": 3,
      "index.number_of_replicas": 3,
      "index.mapping.nested_fields.limit": 50,
      "index.requests.cache.enable": true,
      "index.default_pipeline": "jaeger-ds-service-timestamp",
      "lifecycle": {
        "name": "jaeger-test-policy",
        "rollover_alias": "test-jaeger-service-write"
      }
    },
    "mappings": {
      "dynamic_templates": [
        {
          "span_tags_map": {
            "mapping": {
              "type": "keyword",
              "ignore_above": 256
            },
            "path_match": "tag.*"
          }
        },
        {
          "process_tags_map": {
            "mapping": {
              "type": "keyword",
              "ignore_above": 256
            },
            "path_match": "process.tag.*"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "epoch_millis"
        },
        "serviceName": {
          "type": "keyword",
          "ignore_above": 256
        },
        "operationName": {
          "type": "keyword",
          "ignore_above": 256
        }
      }
    }
  }
}
